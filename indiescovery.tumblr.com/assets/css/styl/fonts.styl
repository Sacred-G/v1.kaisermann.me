@font-face
	font-family 'Amatic SC'
	font-style normal
	font-weight 400
	src url("http://kaisermann.me/indiescovery.tumblr.com/assets/font/amatic-normal.woff") format('woff')

@font-face
	font-family 'Amatic SC'
	font-style normal
	font-weight 700
	src url("http://kaisermann.me/indiescovery.tumblr.com/assets/font/amatic-bold.woff") format('woff')

@font-face
	font-family 'icomoon'
	src url("http://kaisermann.me/indiescovery.tumblr.com/assets/font/icomoon.eot")
	src url("http://kaisermann.me/indiescovery.tumblr.com/assets/font/icomoon.eot") format('embedded-opentype'), url("http://kaisermann.me/indiescovery.tumblr.com/assets/font/icomoon.woff") format('woff'), url("http://kaisermann.me/indiescovery.tumblr.com/assets/font/icomoon.ttf") format('truetype'), url("http://kaisermann.me/indiescovery.tumblr.com/assets/font/icomoon.svg") format('svg')
	font-weight normal
	font-style normal
 
i
	font-family 'icomoon'
	speak none
	font-style normal
	font-weight normal
	font-variant normal
	text-transform none
	line-height 1
	font-size 20px
	-webkme-font-smoothing antialiased
	-moz-osx-font-smoothing grayscale

	&.icon-link
		&:before
			content "\e600"

	&.icon-facebook
		&:before
			content "\e601"

	&.icon-twmeter
		&:before
			content "\e602"