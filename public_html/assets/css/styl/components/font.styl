@font-face {
	font-family: 'Dudu Calligraphy';
	src: url('../font/Dudu_Calligraphy.eot?#iefix') format('embedded-opentype'), 
	url('../font/Dudu_Calligraphy.woff') format('woff'), 
	url('../font/Dudu_Calligraphy.ttf')  format('truetype'),
	url('../font/Dudu_Calligraphy.svg#svgFontName') format('svg');
	font-weight: normal;
	font-style: normal;
}

@font-face {
	font-family: "Flaticon";
	src: url("../font/flaticon.eot");
	src: url("../font/flaticon.eot#iefix") format("embedded-opentype"),
	url("../font/flaticon.woff") format("woff"),
	url("../font/flaticon.ttf") format("truetype"),
	url("../font/flaticon.svg") format("svg");
	font-weight: normal;
	font-style: normal;
}

[class^="flaticon--"]:before,
[class*=" flaticon--"]:before
{   
	font-family: Flaticon;
	font-size: 20px;
	
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.flaticon--wp:before {
	content: "\e000";
}
.flaticon--camera:before {
	content: "\e001";
}
.flaticon--mac:before {
	content: "\e002";
}
.flaticon--github:before {
	content: "\e003";
}
.flaticon--ai:before {
	content: "\e004";
}
.flaticon--guitar:before {
	content: "\e005";
}
.flaticon--ps:before {
	content: "\e006";
}
.flaticon--axure:before {
	content: "\e007";
}
.flaticon--ds:before {
	content: "\e008";
}
.flaticon--music:before {
	content: "\e009";
}
.flaticon--atom:before {
	content: "\e00a";
}
.flaticon--skype:before {
